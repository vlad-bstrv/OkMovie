<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/feedFragment2">

    <fragment
        android:id="@+id/feedFragment2"
        android:name="com.vladbstrv.okmovie.screens.feed.FeedFragment"
        android:label="FeedFragment" >
        <action
            android:id="@+id/action_feedFragment2_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/likesFragment2"
        android:name="com.vladbstrv.okmovie.screens.likes.LikesFragment"
        android:label="LikesFragment" />
    <fragment
        android:id="@+id/settingsFragment2"
        android:name="com.vladbstrv.okmovie.screens.settings.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment2_to_contentProviderFragment"
            app:destination="@id/contentProviderFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.vladbstrv.okmovie.screens.detail.DetailFragment"
        android:label="DetailFragment" />
    <fragment
        android:id="@+id/contentProviderFragment"
        android:name="com.vladbstrv.okmovie.screens.content_provider.ContentProviderFragment"
        android:label="ContentProviderFragment" />
</navigation>